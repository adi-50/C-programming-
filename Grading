#include<stdio.h>
int main()
{
int n;
printf("Enter your marks:-  ");
scanf("%d",&n);
if(n>80 && n<= 100 )
printf("A grade.");
else if(n>=60 && n<80)
printf("B grade .");
else if(n>= 40 && n< 60)
printf(" C grade. ");
else if( n>= 33 && n< 40)
printf(" D grade.");
else 
printf(" Faill !!");
return 0;
}
