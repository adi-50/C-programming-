#include<stdio.h>
int main()
{
char ch;
printf("Enter a Character :-  ");
scanf("%c",&ch);
if(ch>='A' && ch<='Z'|| ch >=  'a' && ch<= 'z'){
if (ch<= 90 ) // using ASCII value of Z = 90
printf("It is a uppercase character.");
else
printf("It is a lower case character.");
}
else
printf(" Invalid output.");
return 0;
}
